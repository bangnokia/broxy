{
    "name": "broxy",
    "version": "1.0.0",
    "description": "Distributed browser-based proxy system",
    "private": true,
    "scripts": {
        "server:install": "cd server && composer install",
        "server:start": "cd server && php start.php start",
        "server:start:daemon": "cd server && php start.php start -d",
        "server:stop": "cd server && php start.php stop",
        "server:restart": "cd server && php start.php restart",
        "server:status": "cd server && php start.php status",
        "ext:pack": "cd extension && zip -r ../broxy-extension.zip . -x '*.git*' -x '*.DS_Store'",
        "dev": "npm run server:start",
        "setup": "npm run server:install"
    },
    "keywords": [
        "proxy",
        "browser",
        "workerman",
        "websocket"
    ],
    "author": "",
    "license": "MIT"
}

